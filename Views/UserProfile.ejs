<!doctype html>
<html lang="en">

<head>
    <!-- Required meta tags -->
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">

    <!-- Bootstrap CSS -->
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-1BmE4kWBq78iYhFldvKuhfTAU6auU8tT94WrHftjDbrCEXSU1oBoqyl2QvZ6jIW3" crossorigin="anonymous">

    <link rel="stylesheet" href="/CSS/navbar.css">
    <link rel="stylesheet" href="/CSS/User.css">
    <link rel="stylesheet" href="/CSS/Userprofile.css">

    <title>User Interface</title>
</head>

<body>


    <!-- Nav Bar -->
    <nav>
        <input type="checkbox" id="check">
        <label for="check" class="checkbtn">
      <i class="fa fa-bars"></i>
    </label>
        <label for="" class="logo">Master-U</label>
        <ul>
            <li class="active"><a href="/home_1" class="nav_item">Home</a></li>
            <li><a href="/home_1" class="nav_item">Exams</a></li>
            <li><a href="/HTML/scroll.html" class="nav_item">Updates</a></li>
            <li><a href="/HTML/faq.html" class="nav_item">FAQs</a></li>
            <li><a href="/HTML/aboutus.html" class="nav_item">About</a></li>
            <li><a href="/Contactus" class="nav_item">Contact Us</a></li>
            <li>
                <a href="/dir_profile"><img src="/IMAGES/user-icon.png" alt="" width="38px"></a>
            </li>
        </ul>
    </nav>


    <div id="div1">
        <div class="container">
            <div class="card">
                <div class="card-body">
                    <h5 class="card-title" style="text-decoration: underline;">Profile</h5>
                    <br><br><br>

                    <table class="table table-success table-striped" id="table">
                        <tr id="row1">
                            <th>UserName</th>
                            <th>Email</th>
                            <th>Dob</th>
                            <th>Profession</th>
                            <th>Additional Info</th>
                        </tr>
                        <tr id="row2">
                            <td>
                                <%= data.username %>
                            </td>
                            <td>
                                <%= data.email %>
                            </td>
                            <td>
                                <%= data.dob %>
                            </td>
                            <td>
                                <%= data.profession %>
                            </td>
                            <td>
                                <%= data.info %>
                            </td>
                        </tr>
                    </table>

                    <a href="/HTML/UserDetails.html" class="card-link btn-danger butn">Edit Details</a>
                    <a href="/" class="card-link btn-danger butn">Log Out</a>
                </div>
            </div>
        </div>
    </div>











    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/js/bootstrap.bundle.min.js" integrity="sha384-ka7Sk0Gln4gmtz2MlQnikT1wXgYsOg+OMhuP+IlRH9sENBO0LRn5q+8nbTov4+1p" crossorigin="anonymous"></script>
</body>

</html>